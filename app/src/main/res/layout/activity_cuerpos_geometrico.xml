                android:layout_marginTop="20dp"
                android:padding="10dp"
                android:textColor="@color/red_primary_dark"
                android:textStyle="bold"
                android:textSize="20sp"
                android:fontFamily="@font/muli"
                android:textAlignment="center"
                android:text="Un cuerpo geométrico es una figura geométrica de tres dimensiones. Tiene largo, ancho y alto."
                app:layout_constraintEnd_toEndOf="@id/guideline_percent"
                app:layout_constraintStart_toStartOf="@id/guideline_percent"
                app:layout_constraintTop_toBottomOf="@+id/textView79"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <LinearLayout
                android:id="@+id/linearlayout_propiedad_uno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:orientation="horizontal"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/text">

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="5dp">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/monserratalternates_bold"
                            android:text="Poliedros"
                            android:textStyle="bold"
                            android:textAlignment="center"
                            android:textSize="20sp" />


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/muli"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp"
                            android:text="Son cuerpos geométricos cerrados, limitados por caras poligonales."
                            android:textAlignment="center"
                            android:textSize="17sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/muli"
                            android:paddingTop="5dp"
                            android:textColor="@color/black"
                            android:paddingBottom="5dp"
                            android:text="Elementos"
                            android:textAlignment="center"
                            android:textSize="17sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/muli"
                            android:textColor="@color/black"
                            android:text="Caras: Polígonos que limitan al poliedro."
                            android:textAlignment="center"
                            android:textSize="17sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/muli"
                            android:textColor="@color/black"
                            android:text="Aristas: Segmentos intersección de las caras."
                            android:textAlignment="center"
                            android:textSize="17sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/muli"
                            android:textColor="@color/black"
                            android:text="Vértices: Puntos de intersección de las aristas."
                            android:textAlignment="center"
                            android:textSize="17sp"
                            android:textStyle="bold" />

                        <View
                            android:background="#DFDFDF"
                            android:layout_width="match_parent"
                            android:layout_height="4dp"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/monserratalternates_bold"
                            android:text="Cuerpos redondos"
                            android:textStyle="bold"
                            android:textAlignment="center"
                            android:textSize="20sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/muli"
                            android:paddingTop="10dp"
                            android:paddingBottom="10dp"
                            android:text="Son cuerpos geométricos limitados total o parcialmente por superficies curvas. Dentro de éstos están los cuerpos de revolución, que son cuerpos redondos que se obtienen al girar una figura plana alrededor de un eje."
                            android:textAlignment="center"
                            android:textSize="17sp"
                            android:textStyle="bold" />


                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    app:cardCornerRadius="5dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="5dp">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/monserratalternates_bold"
