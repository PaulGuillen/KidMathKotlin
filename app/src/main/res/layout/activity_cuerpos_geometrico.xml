                            android:textAlignment="center"
                            android:textSize="20sp" />


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:fontFamily="@font/monserratalternates_regular"
                            android:maxLines="3"
                            android:paddingTop="10dp"
                            android:text="Los poliedros son denominados de acuerdo a su número de caras. Su designación se basa en el griego clásico."
                            android:paddingBottom="10dp"
                            android:textAlignment="center"
                            android:textSize="17sp"
                            android:textStyle="bold" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:textColor="@color/black"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Nombre"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Tetraedro"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Pentaedro"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Hexaedro"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Heptaedro"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Octaedro"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Eneadro"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Decaedro"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Endecaedro"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Dodecaedro"
                                    android:textAlignment="center"
                                    android:textSize="16sp" />
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Tridecaedro"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="0.6"
                                android:orientation="vertical">

                                <TextView
                                    android:textColor="@color/black"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="Caras"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="4"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="5"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="6"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="7"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="8"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="9"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="10"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="11"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="12"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/monserratalternates_bold"
                                    android:text="13"
                                    android:textAlignment="center"
                                    android:textSize="17sp" />



                            </LinearLayout>

                        </LinearLayout>

                        <TextView
